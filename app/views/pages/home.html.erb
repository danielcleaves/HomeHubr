	<%= form_tag search_path, method: :get do %>
		<div class="row">
			<div class="col-md-8">
				<%= text_field_tag :search, params[:search], placeholder: "Search address, city, or state", class: "form-control", id:"autolocation"%>
			</div>
			<div class= "col-md-4">
				<%= submit_tag "Search", class: "btn btn-normal btn-block" %>
			</div>
		</div>
	<% end %>

