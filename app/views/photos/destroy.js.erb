$('#photos').html("<%= render 'photos_list' %>")
<% if @house.photos.blank? %>
	$('#photo_check').hide();
<% end %>

<% if is_ready = !@house.price.blank?  && !@house.listing_name.blank? &&!@house.photos.blank?  &&!@house.address.blank? %>
	$('#publish_button').attr('disabled', false);
<% else %>
	$('#publish_button').attr('disabled', true);
<% end %>